	<template id="layout_angular_material" name="layout para angula.material">
		<html ng-app="MySecondApp">
			<head>
				<t t-call="website.assets_frontend"/>
			</head>
			<header> 
				<div ng-controller="FirstController" ng-cloak="" class="gridListdemoBasicUsage">
			   	<md-grid-list md-cols-xs="1" md-cols-sm="2" md-cols-md="4" md-cols-gt-md="6" md-row-height-gt-md="1:1" md-row-height="2:2" md-gutter="12px" md-gutter-gt-sm="8px" class="gridListdemoBasicUsage gray">
						 <strong>Welcome</strong> in our websiteeee!
			        {{nombre}}
			        {{apellido}}
			       
			        
			         <md-input-container>
				        <label>Nombre</label>
				        <input ng-model="nombre"/>
				     </md-input-container>

						 </md-grid-list>
			     </div>
			</header>

		</html>
	</template>

	<template id="footer_inherit" inherit_id="website.footer_default">
		<xpath expr="//div[@id='footer']" position="replace">
			<div id="footer" class="container hidden-print">
				
			</div>
			
		</xpath>
	</template>

	<template id="editor_head_jpv" inherit_id="website.user_navbar" name="Editor">
    
    <xpath expr='//nav[@id="oe_main_menu_navbar"]' position="attributes">
        <attribute name="class">hidden</attribute>
    </xpath>
    
</template>


<template id="SideNavMenu" name="SideNav Left">
	<div ng-controller="SideNavController" layout="column" style="height: 500px;" ng-cloak="">
		  <!-- <section layout="row" flex=""> -->
		    <md-sidenav class="md-sidenav-left" md-component-id="left"
		                md-disable-backdrop="" md-whiteframe="4">
		      <md-toolbar class="md-theme-indigo md-warn">
		        <div><h1 class="md-toolbar-tools">Men√∫</h1></div>
		      </md-toolbar>
		      <md-content layout-margin="">
		        <p>
		          This sidenav is not showing any backdrop, where users can click on it, to close the sidenav.
		        </p>
		      </md-content>
		      		<div layout="row" layout-align="top right" ng-controller="SideNavController">
		     	 	<div>
			        <md-button ng-click="toggleLeft()" class="md-icon-button md-warn">
			          <md-icon md-svg-src="angular_material_integration/static/src/app/img/icons/ic_arrow_back_24px.svg"></md-icon>
			        </md-button>
		    		</div>
				</div>
		    </md-sidenav>
		  <!-- </section> -->
		</div>
</template>
<template id="Toolbar">
	<div ng-controller="ToolbarController" layout="column" ng-cloak="" class="">
	<div class="header md-accent-bg" flex="">
	  <md-toolbar md-scroll-shrink="" ng-if="true" class="md-warn md-hue-3" style="background-color: transparent;" ng-controller="TitleController">
	    <div class="md-toolbar-tools">
	    	<div layout="column" layout-align="top left" ng-controller="SideNavController">
		      <div>
		          <md-button ng-click="toggleLeft()" aria-label="menu" class="md-fab md-warn">
		            <md-icon md-svg-src="angular_material_integration/static/src/app/img/icons/menu.svg"></md-icon>
		          </md-button>
		        </div>
		      </div>
	      <h3 class="md-flex">
	        <span>{{title}}</span>
	      </h3>
	    </div>
	  </md-toolbar>
	</div>
	<md-content flex=""  md-scroll-y="" style="height:600px">
